<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <OutputType>Library</OutputType>
        <IncludeOpenAPIAnalyzers>true</IncludeOpenAPIAnalyzers>
        <Nullable>enable</Nullable>
        <RootNamespace>Caravel</RootNamespace>
        <IsPackable>true</IsPackable>
        <PackageVersion>0.1.1</PackageVersion>
        <RepositoryUrl>https://github.com/Talento90/Caravel</RepositoryUrl>
        <PackageTags>dotnetcore,sdk</PackageTags>
        <LangVersion>8</LangVersion>
        <Title>Caravel</Title>
        <Authors>Marco Talento</Authors>
        <Description>Caravel is a package that contains reusable code.</Description>
    </PropertyGroup>

</Project>
