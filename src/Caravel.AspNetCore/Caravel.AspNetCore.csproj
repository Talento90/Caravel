<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <OutputType>Library</OutputType>
        <IncludeOpenAPIAnalyzers>true</IncludeOpenAPIAnalyzers>
        <Nullable>enable</Nullable>
        <IsPackable>true</IsPackable>
        <PackageVersion>0.2.2</PackageVersion>
        <RepositoryUrl>https://github.com/Talento90/Caravel</RepositoryUrl>
        <PackageTags>dotnetcore,sdk</PackageTags>
        <LangVersion>8</LangVersion>
        <Title>Caravel.AspNetCore</Title>
        <Authors>Marco Talento</Authors>
        <Description>Caravel is a package that contains common utilities for building asp.net core applications.</Description>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\Caravel\Caravel.csproj" />
    </ItemGroup>
    
</Project>
